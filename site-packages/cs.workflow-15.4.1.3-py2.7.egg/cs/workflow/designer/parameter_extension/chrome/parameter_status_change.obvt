<div class="wf-parameter-status-change" data-call="checkStyle">
<a class="btn btn-mini wf-remove pull-right" data-on="click|remove" data-if="!readonly"><img src="${options.localres}trash.png" /></a>
<a class="btn btn-mini rule-action"><img src="${options.plugins}catalog/catalog.png" data-if="!readonly" /></a>
<div class="parameter-rule text-ellipsis pull-right" title="{rule_name}">{rule_name}</div>
<div class="parameter-text editable" data-call="checkFilter" title="{target_state}">{target_state}</div>
<div class="no-float"></div>
</div>
