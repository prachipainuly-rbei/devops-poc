<div class="wf-briefcase-links">
    <div class="briefcase-dropdown dropup pull-right" data-if="addUrl">
        <a class="dropdown-toggle btn btn-mini" role="button" data-toggle="dropdown" data-target="#">
            <img data-src="${options.localres}new.png" />
        </a>
        <ul class="dropdown-menu" role="menu" data-on="click|addBriefcaseLink">
        </ul>
    </div>
    <div class="title-icon">
        <img class="svg-icon" data-src="${icon}" />
    </div>
    <div class="title-text text-ellipsis muted" title="${label}">${label}</div>
    <div class="no-float"></div>
    <ul class="unstyled">
        <li data-repeat="links" data-render="@."></li>
    </ul>
</div>
