<div>
    <h3>${label}</h3>
    <ul class="unstyled">
        <li class="wf-briefcase" data-repeat="briefcases" data-render="@."></li>
    </ul>
    <div class="wf-add-breifcase" data-if="addUrl">
        <input type="text" name="name" class="pull-left" data-on="keypress|popupTypeSelector" />
        <div class="dropdown pull-left">
            <a class="dropdown-toggle btn btn-small" role="button" data-toggle="dropdown" data-target="#">
                <div class="wf-icon-new"></div>
            </a>
            <ul class="briefcaselink-meanings dropdown-menu pull-right" role="menu" data-on="click|addBriefcase">
            </ul>
        </div>
        <div class="no-float"></div>
    </div>
</div>
