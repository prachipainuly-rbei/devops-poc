<li class="nav-header">${heading}</li>
<tal:tasktypes repeat="ttype task_types">
    <li><a href="#" data-task-type="${ttype.type}">${ttype.label}</a></li>
    <tal:taskexts repeat="extension ttype.extensions">
        <li><a href="#" class="wf-task-extension" data-task-type="${ttype.type}" data-task-extension="${extension.type}">${extension.label}</a></li>
    </tal:taskexts>
</tal:tasktypes>
<li class="divider"></li>
<li class="nav-header">${heading_systemtask}</li>
<tal:tasktypes repeat="ttype systemtask_types">
    <li><a href="#" data-task-type="${ttype.type}" data-task-definition="${ttype.id}">${ttype.label}</a></li>
</tal:tasktypes>
