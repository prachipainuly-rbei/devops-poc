<div class="task-info">
    <div class="system-task-info">
        <div class="type-info pull-right">
            <div class="title-icon">
                <img class="svg-icon" data-if="icon" data-src="{icon}" />
            </div>
            <div class="title-text text-ellipsis" title="{type_desc}">{type_desc}</div>
        </div>
        <div class="systemtask-icon pull-left">
            <img data-if="picture" data-src="{picture}" />
            <img data-if="!picture" data-src="${options.localres}systemtask.png" />
        </div>
        <div class="info-parameters row-fluid" data-render="parameters" data-if="parameters.parameters"></div>
    </div>
</div>
