<div class="task-info">
    <div class="type-info pull-right">
        <div class="title-icon">
            <img class="svg-icon" data-src="{icon}" data-if="icon" />
        </div>
        <div class="title-text text-ellipsis" title="{type_desc}">{type_desc}</div>
    </div>
    <div class="info-responsible row-fluid" data-render="responsible"></div>
</div>
