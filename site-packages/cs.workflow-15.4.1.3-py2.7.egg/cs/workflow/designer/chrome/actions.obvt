<div class="btn-group">
    <button class="btn zoom-in" title="${label_zoom_in}">
        <img data-src="${options.localres}zoom_in.png"/>
    </button>
    <button class="btn zoom-out" title="${label_zoom_out}">
        <img data-src="${options.localres}zoom_out.png"/>
    </button>
    <button class="btn zoom-all" data-toggle="button" title="${label_zoom_all}">
        <img data-src="${options.localres}zoom_all.png"/>
    </button>
</div>
<div class="btn-group">
    <button
        class="btn start-workflow"
        title="${label_start}"
        data-if="startUrl"
    >
        <img data-src="/resources/icons/byname/cdbwf_start_workflow" />
    </button>
    <button
        class="btn hold-workflow"
        title="${label_onhold}"
        data-if="onHoldUrl"
    >
        <img data-src="/resources/icons/byname/cdbwf_onhold_workflow" />
    </button>
    <button
        class="btn cancel-workflow"
        title="${label_cancel}"
        data-if="cancelUrl"
    >
        <img data-src="/resources/icons/byname/cdbwf_cancel_workflow" />
    </button>
    <button
        class="btn dismiss-workflow"
        title="${label_dismiss}"
        data-if="dismissUrl"
    >
        <img data-src="/resources/icons/byname/cdbwf_dismiss_workflow" />
    </button>
</div>
<div class="btn-group">
    <button
        class="btn add-task"
        title="${label_add_task}"
        data-if="addTask"
        data-on="click|selectTask"
    >
        <img data-src="${options.localres}new.png" />
    </button>
    <button
        class="btn add-completion"
        title="${label_add_completion}"
        data-if="addCompletion"
        data-on="click|addCompletionTask"
    >
        <img data-src="${options.localres}workflow_completion.png" />
    </button>
    <button
        class="btn save-as-template"
        title="${label_save_as_template}"
        data-if="saveAsTemplateUrl"
    >
        <img data-src="${options.localres}save_as_template.png" />
    </button>
</div>
<div class="btn-group">
    <button
        class="btn modify-workflow"
        title="{modifyOperation.tooltip}"
        data-if="modifyOperation"
    >
        <img class="svg-icon" data-src="{modifyOperation.icon}" />
    </button>
    <a
        href="{protocol.url}"
        data-if="protocol"
        class="btn"
        title="{protocol.tooltip}"
    >
        <img data-src="{protocol.img}" alt="" />
    </a>
</div>
