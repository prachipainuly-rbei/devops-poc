<div>
    <h3>${label}</h3>
    <ul class="unstyled">
        <li class="wf-briefcase" data-repeat="briefcases" data-render="@." data-briefcase-id="{briefcase_id}"></li>
    </ul>
    <div class="wf-add-breifcase" data-if="addUrl">
        <input type="text" name="name" class="pull-left" data-on="keypress|createOnEnter"/>
        <a class="btn btn-small" role="button" data-on="click|addBriefcase">
            <div class="wf-icon-new"></div>
        </a>
        <div class="no-float"></div>
    </div>
</div>
