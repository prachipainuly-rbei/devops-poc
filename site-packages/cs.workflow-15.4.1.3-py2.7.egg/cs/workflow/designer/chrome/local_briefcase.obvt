<a class="btn btn-mini wf-remove pull-right" role="button" data-on="click|remove" data-if="deleteUrl">
    <img data-src="${options.localres}trash.png" />
</a>
<a class="btn btn-mini toggle-details pull-right" data-toggle="collapse" role="button" data-call="checkToggleButton">
    <i class="icon-chevron-down"></i>
    <i class="icon-chevron-up disabled"></i>
</a>
<div class="title-icon">
    <img class="svg-icon" data-src="{icon}" data-if="icon" />
</div>
<h4 class="editable briefcase-name text-ellipsis" title="{name}">{name}</h4>
<ul class="unstyled wf-briefcase-contents collapse" data-call="checkDocumentExistance">
    <li data-repeat="contents" data-render="@."></li>
</ul>
