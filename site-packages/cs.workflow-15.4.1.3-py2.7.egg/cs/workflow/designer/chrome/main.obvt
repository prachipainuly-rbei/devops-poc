<div class="wf-designer-toolbar unselectable">
    <div class="wf-message-container">
        <div class="wf-error-message row-fluid"></div>
    </div>
    <div class="wf-process-title pull-left">
        <div class="muted">${label_title}</div>
        <div class="heading3">
            <div class="title-icon pull-left" title="{cdb_status_txt}">
                <img class="svg-icon" data-src="{icon}" data-if="icon" />
            </div>
            <div class="editable title-text pull-left" title="{title}">{title}</div>
        </div>
    </div>
    <div class="wf-actions pull-left" data-render="actions"></div>
    <div class="no-float"></div>
    <div class="wf-details">
        <div class="detail-collapse collapse">
            <div class="wf-process-project pull-left" data-if="pcs_installed">
                <div class="muted">${label_project}</div>
                <div data-render="project"></div>
                <div class="no-float"></div>
            </div>
            <div class="wf-process-description pull-left" data-render="description"></div>
        </div>
        <div class="detail-actions">
            <button class="btn btn-mini toggle-details" data-toggle="collapse">
                <i class="icon-chevron-down"></i>
                <i class="icon-chevron-up disabled"></i>
            </button>
        </div>
    </div>
  </div>
  <div class="row-fluid unselectable">
    <div class="wf-viewport-margin span9 row-fluid">
        <div class="span12 wf-viewport">
            <div class="wf-process" data-render="graph"></div>
            <div class="no-float"></div>
        </div>
    </div>
    <div class="span3 wf-sidebar-panel" data-render="sidebar">
    </div>
</div>
