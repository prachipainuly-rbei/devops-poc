<div class="title-text text-ellipsis muted" title="${default_label}" data-if="!label">${default_label}</div>
<div class="title-text text-ellipsis muted" title="{label}" data-if="label">{label}</div>
<ul class="unstyled parameter-list {plist_class}" data-if="parameters">
    <li data-repeat="parameters" data-render="@."></li>
</ul>
<a
    href="#"
    title="${label_more}"
    class="button-more-parameters"
    data-if="!showAllParameters"
    data-on="click|expandDetails"
>
    ${label_more}&rarr;
</a>
