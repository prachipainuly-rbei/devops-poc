<div class="wf-constraint no-float">
    <a class="btn btn-mini wf-remove pull-right" role="button" data-on="click|remove" data-if="deleteUrl">
        <img data-src="${options.localres}trash.png" />
    </a>
    <div class="text-ellipsis" title="{rule_name}">{rule_name}</div>
    <div class="no-float constraint-action row-fluid">
        <div class="span4 constraint-invert">
            <label class="checkbox">
                <input type="checkbox" data-call="checkCheckbox" title="${label_invert}">
                <img data-src="${options.localres}invert.png" title="${label_invert}" />
            </label>
        </div>
        <div class="span8">
            <div data-unwrap="" data-if="modifyUrl">
                <div class="dropdown briefcase-dropdown pull-right">
                    <a class="btn btn-mini dropdown-toggle" role="button" data-toggle="dropdown" data-target="#">
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu" role="menu" data-if="modifyUrl"></ul>
                </div>
                <div class="muted text-ellipsis" data-if="!briefcase_name" title="${label_briefcase}">${label_briefcase}</div>
            </div>
            <div class="text-ellipsis" data-if="briefcase_name" title="{briefcase_name}">{briefcase_name}</div>
        </div>
    </div>
</div>
