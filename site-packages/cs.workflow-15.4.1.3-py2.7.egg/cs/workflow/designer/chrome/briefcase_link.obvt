<div class="wf-briefcase-link" data-briefcase-id="{briefcase_id}">
    <a class="btn btn-mini wf-remove pull-right" role="button" data-on="click|remove" data-if="deleteUrl">
        <img data-src="${options.localres}trash.png" />
    </a>
    <div class="btn-group pull-right wf-mode-buttons" data-toggle="buttons-radio" data-if="modifyUrl">
        <a
            class="btn btn-mini"
            role="button"
            title="${info_mode_label}"
            data-on="click|changeMeaning"
            data-meaning="info"
            data-call="checkModeButton"
        >
            <img data-src="${options.localres}info.png" />
        </a>
        <a
            class="btn btn-mini"
            role="button"
            title="${edit_mode_label}"
            data-on="click|changeMeaning"
            data-meaning="edit"
            data-call="checkModeButton"
        >
            <img data-src="${options.localres}edit.png" />
        </a>
    </div>
    <div class="title-icon" title="{meaning}">
        <img class="svg-icon" data-src="{icon}" data-if="icon" />
    </div>
    <div class="title-text text-ellipsis" title="{briefcase_name}">{briefcase_name}</div>
    <div class="no-float"></div>
</div>
