<div class="wf-briefcase-content">
    <a class="btn btn-mini wf-remove pull-right" role="button" data-on="click|remove" data-if="deletable">
        <img data-src="${options.localres}trash.png" />
    </a>
    <div class="wf-operations pull-right" data-if="operations" data-render="operations"></div>
    <div class="title-icon">
        <img class="svg-icon" data-src="{icon}" data-if="icon"/>
    </div>
    <div class="title-text text-ellipsis" title="{description}">
        <a href="{url}">{description}</a>
    </div>
    <div class="no-float"></div>
</div>
