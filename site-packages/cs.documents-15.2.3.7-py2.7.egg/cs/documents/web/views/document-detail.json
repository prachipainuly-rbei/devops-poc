{
    "configuration": {
        "name": "cs-web-components-base-SimpleLayout",
        "children": [
            {
                "name": "cs-documents-web-OperationModalWithFiles",
                "properties": {
                    "handlerName": "modal-with-files"
                }
            },
            {"name": "cs-web-components-base-ObjectApplicationBar"},
            {
                "name": "cs-web-components-base-FixedSidebarWithEditor",
                "properties": {
                    "masterDataConfig": {
                        "groupId": "document",
                        "blockId": "documentMasterData"
                    },
                    "attributeDialog": "dokument_mask_webattrs"
                },
                "components": {
                    "content": {
                        "name": "cs-web-components-base-FileSelectionProvider",
                        "children": [
                            {
                                "name": "cs-web-components-base-ChannelListener",
                                "children": [
                                    {
                                        "name": "cs-web-components-base-ObjectContent",
                                        "properties": {
                                            "key": "objectContent"
                                        }
                                    }
                                ],
                                "properties": {
                                    "channels": [
                                        {
                                            "channelName": "fileSelection",
                                            "channelMapping": {
                                                "selectedFile": "selection"
                                            }
                                        },
                                        {
                                            "channelName": "sidebar",
                                            "channelMapping": {
                                                "minified": "isEditing"
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "name": "cs-web-components-base-ChannelListener",
                                "children": [
                                    {
                                        "name": "cs-web-components-base-FileList"
                                    }
                                ],
                                "properties": {
                                    "channels": [
                                        {
                                            "channelName": "fileSelection",
                                            "channelMapping": {
                                                "onSelect": "onSelect"
                                            }
                                        },
                                        {
                                            "channelName": "sidebar",
                                            "channelMapping": {
                                                "minified": "isEditing"
                                            }
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                },
                "children": [
                    {
                        "name": "cs-web-components-base-RelatedObjects"
                    }
                ]
            },
            {
                "name": "cs-web-components-base-OperationScope",
                "children": [
                    {
                        "name" : "cs-web-components-base-RelationshipGroup",
                        "properties": {
                            "exclude": ["Files"]
                        }
                    },
                    {
                        "name": "cs-web-components-base-OperationModal",
                        "properties": {
                            "handlerName": "editor"
                        }
                    }
                ]
            },
            {
                "name": "cs-activitystream-web-ObjectActivitiesBlock"
            }
        ]
    },
    "setupFunctions": [
        "cs.documents.web.add_document_create"
    ],
    "libraries": [
        ["cs-activitystream-web", "15.1.0"],
        ["cs-documents-web", "15.3.0"]
    ],
    "pluginContexts": [
        "content-view",
        "class-tile-small"
    ]
}
