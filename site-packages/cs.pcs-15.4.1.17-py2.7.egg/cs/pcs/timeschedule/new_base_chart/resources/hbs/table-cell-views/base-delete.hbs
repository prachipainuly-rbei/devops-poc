{{#unless view.value.unremovable}}
	{{#if view.disabledSchedule}}
			{{#if view.value.inSchedule}}
			    <img class="clickable icon-button readonly"
			        {{bind-attr src=view.srcPinActive}}
			        {{bind-attr title=controller.labels.pcs_resource_fixed}}
	        >
			{{else}}
			    <img class="clickable icon-button readonly"
			        {{bind-attr src=view.srcPinNotActiveTransparent}}
			        {{bind-attr title=controller.labels.pcs_resource_not_fixed}}
	        >
			{{/if}}
	{{else}}
			{{#if view.value.inSchedule}}
			   <img class="clickable icon-button"
			        {{bind-attr src=view.srcPinActive}}
			        {{bind-attr title=controller.labels.pcs_resource_fixed}}
			        {{action "toggleInSchedule" bubbles=false on="click" target="view"}}
	       >
			{{else}}
			   <img class="clickable icon-button"
			        {{bind-attr src=view.srcPinNotActive}}
			        {{bind-attr title=controller.labels.pcs_resource_not_fixed}}
			        {{action "toggleInSchedule" bubbles=false on="click" target="view"}}
	       >
			{{/if}}
	{{/if}}
{{/unless}}