<div class="schedule-table-header">
  <div class="schedule-table-header-column order">
    &nbsp;
  </div>
  {{#each column in settings.columns}}
    <div {{bind-attr class=":schedule-table-header-column column.format column.name"}}
    {{bind-attr style=column.columnStyle}}
    {{bind-attr name=column.name}}
    {{bind-attr title=column.title}}>
      <div class="header-column-text">
        {{column.title}}
        &nbsp;
      </span>
    </div>
  {{/each}}
</div>
<div class="schedule-table-body">
  <div class="dummy" style="height: 0px">
  </div>
  {{#each row in model.rows}}
    {{base-schedule-table-row settings=settings model=row chartType=chartType refTable=model}}
  {{/each}}
</div>
