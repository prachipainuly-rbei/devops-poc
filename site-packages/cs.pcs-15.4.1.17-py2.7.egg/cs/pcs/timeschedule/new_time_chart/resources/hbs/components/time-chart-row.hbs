{{#if viewingContent}}
  {{#if viewingContent.scheduleContent.early_start}}
    <i {{bind-attr class=leftIntervalBorderClass}} {{bind-attr style=leftIntervalBorderStyle}}></i>
  {{/if}}
  
  {{time-chart-bar settings=settings model=model}}
  {{#if displayPlanBar}} 
    {{time-chart-plan-bar settings=settings model=model}}
  {{/if}}
  
  {{#if viewingContent.scheduleContent.late_finish}}
    <i {{bind-attr class=rightIntervalBorderClass}} {{bind-attr style=rightIntervalBorderStyle}}></i>
  {{/if}}
  
  {{time-chart-relations settings=settings model=model refTable=refTable}}
{{/if}}