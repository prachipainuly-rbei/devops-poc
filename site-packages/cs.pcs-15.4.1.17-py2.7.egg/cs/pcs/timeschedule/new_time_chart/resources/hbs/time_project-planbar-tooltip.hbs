{{#if model}}
    <ul class="chart-tooltip dropdown-menu popover">
      <div class="arrow"></div>
      <li><a>{{labels.cdbpcs_project}}: {{model.scheduleContent.title}}</a></li>
      <li><a>{{model.scheduleContent.startPlanDateStr}} - {{model.scheduleContent.endPlanDateStr}}</a></li>
      <li><a>{{model.scheduleContent.duration_plan}}
          {{#if model.scheduleContent.multiplePlanWorkdays}}
              {{labels.workdays_label}}
          {{else}}
              {{labels.workday_label}}
          {{/if}}
           </a></li>
    </ul>
{{/if}}
