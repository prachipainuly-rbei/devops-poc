{{#unless viewingContent.scheduleContent.isMilestone}}
  <div class="progress-completion" {{bind-attr style=percentCompleteStr}}></div>
{{/unless}}
{{#if viewingContent.scheduleContent.barWidth}}
  <span class="text" {{bind-attr style=textStyle}}>
    {{viewingContent.scheduleContent.barDescription}}
  </span>
{{/if}}
{{#if viewingContent.scheduleContent.end_plan_fix}}
  <i class="icon-circle end_plan_fix" {{bind-attr title=labels.pcs_end_fix}}></i>
{{/if}} 