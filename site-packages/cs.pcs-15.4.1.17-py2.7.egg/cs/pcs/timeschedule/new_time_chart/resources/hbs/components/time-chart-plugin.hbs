<div class="time-chart">
  {{time-chart-top settings=settings model=model}}
  <div class="bottom-panel">
    {{#base-chart-left-panel settings=settings model=model chartType="time"}}
      {{render 'timeRelationTooltip'}}
    {{/base-chart-left-panel}}
    {{#base-chart-right-panel settings=settings model=model}}
      {{time-chart settings=settings model=model}}
    {{/base-chart-right-panel}}
  </div>
</div>
