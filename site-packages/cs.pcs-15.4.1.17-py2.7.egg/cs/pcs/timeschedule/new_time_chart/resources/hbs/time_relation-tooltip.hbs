{{#if view.tempTooltipObj}}
	<ul class="relation-tooltip dropdown-menu popover right" {{bind-attr style=view.style}}>
	  <div class="arrow"></div>
	  <li><a>{{view.classLabel}}: {{view.tempTooltipObj.scheduleContent.title}}</a></li>
    <li><a>{{view.tempTooltipObj.scheduleContent.startDateStr}} - {{view.tempTooltipObj.scheduleContent.endDateStr}}</a></li>
    <li><a>{{view.tempTooltipObj.scheduleContent.duration}}
        {{#if view.tempTooltipObj.scheduleContent.multipleWorkdays}}
            {{labels.workdays_label}}
        {{else}}
            {{labels.workday_label}}
        {{/if}}
    </a></li>
	</ul>
{{/if}}
