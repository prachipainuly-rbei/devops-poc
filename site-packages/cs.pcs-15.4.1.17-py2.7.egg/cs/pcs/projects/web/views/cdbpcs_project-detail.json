{
  "configuration": {
    "name": "cs-pcs-projects-web-DetailLayout",
    "children": [
      {
        "name": "cs-web-components-base-ObjectApplicationBar"
      },
      {
        "name": "cs-pcs-projects-web-ProjectInfoTile"
      },
      {
        "name": "cs-pcs-projects-web-ProjectRelships",
        "properties": {
          "relships": ["ProjectDocuments", "AllTasks", "OpenIssues", "Cdbpcs_project_to_cdb_person"]
        },
        "children": [
          {
            "name": "cs-pcs-projects-web-Slider",
            "properties": {
              "scrollPaneTwo": true
            },
            "children": [
              {
                "name": "cs-web-components-base-ContextObjectSetter",
                "properties": {
                  "setPath": "cs-pcs-projects-web-ProjectRelshipTable-ProjectDocuments"
                },
                "children": [
                  {
                    "name": "cs-pcs-projects-web-ProjectRelshipTable",
                    "properties": {
                      "relshipName": "ProjectDocuments"
                    }
                  }
                ]
              },

              {
                "name": "cs-web-components-base-ContextObjectWrapper",
                "properties": {
                  "readPath": "cs-pcs-projects-web-ProjectRelshipTable-ProjectDocuments"
                },
                "children": [
                  {
                    "name": "cs-web-components-base-ObjectContent",
                    "properties": {
                      "minified": true
                    }
                  }
                ]
              }
            ]
          },
          {
            "name": "cs-pcs-projects-web-ProjectRelshipTable",
            "properties": {
              "relshipName": "AllTasks",
              "asTree": true,
              "treeColumnID": "cdb::argument::tablecolumn.task_name",
              "treeParentColumnID": "cdb::argument::tablecolumn.parent_task",
              "treeIDColumnID": "cdb::argument::tablecolumn.task_id"
            }
          },
          {
            "name": "cs-pcs-projects-web-ProjectRelshipTable",
            "properties": {
              "relshipName": "OpenIssues"
            }
          },
          {
            "name": "cs-pcs-projects-web-ProjectRelshipTable",
            "properties": {
              "relshipName": "Cdbpcs_project_to_cdb_person"
            }
          }
        ]
      }
    ]
  },
  "pluginContexts": [
        "content-view"
    ],
  "libraries": [
    ["cs-documents-web", "0.1.0"],
    [
      "cs-pcs-projects-web",
      "15.1.0"
    ]
  ]
}
