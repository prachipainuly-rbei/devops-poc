<div class="tree-nav-action prev btn btn-mini btn-link" {{action "jumpToPrev" treeSearch}}>
    <i class="icon-step-backward"></i>
</div>
<div class="tree-nav-info btn btn-mini btn-link"  {{action "jumpToCurrent" treeSearch}}>
    {{treeSearch.navIdxPlusOne}} / {{treeSearch.content.length}}
</div>
<div class="tree-nav-action next btn btn-mini btn-link" {{action "jumpToNext" treeSearch}}>
    <i class="icon-step-forward"></i>
</div>
